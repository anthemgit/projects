<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="ExtractVariables.ErrorDetails">
    <DisplayName>ExtractVariables.ErrorDetails</DisplayName>
    <FaultRules/>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <JSONPayload>
        <Variable name="code">
            <JSONPath>$.additionalErrorInformation[0].code</JSONPath>
        </Variable>
        <Variable name="message">
            <JSONPath>$.additionalErrorInformation[0].message</JSONPath>
        </Variable>
        <Variable name="description">
            <JSONPath>$.additionalErrorInformation[0].description</JSONPath>
        </Variable>
    </JSONPayload>
    <Source clearPayload="true">response.content</Source>
    <VariablePrefix>err</VariablePrefix>
</ExtractVariables>
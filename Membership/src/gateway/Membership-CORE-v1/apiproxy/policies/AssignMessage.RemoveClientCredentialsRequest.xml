<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.RemoveClientCredentialsRequest">
    <DisplayName>AssignMessage.RemoveClientCredentialsRequest</DisplayName>
    <FaultRules/>
    <Properties/>
    <Remove>
        <Headers>
          <Header name="X-ANTHEM-CLIENT-ID" />
          <Header name="X-ANTHEM-CLIENT-SECRET" />
          <Header name="Authorization" />
        </Headers>
    </Remove>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.GetAppCredentialsRequest">
    <DisplayName>AssignMessage.GetAppCredentialsRequest</DisplayName>
    <FaultRules/>
    <Properties/>
    <Add>
      <Headers>
        <Header name="X-REQUEST-ID">{messageid}</Header>
      </Headers>
    </Add>
    <AssignVariable>
        <Name>anthem.clientId</Name>
      <Ref>request.header.X-ANTHEM-CLIENT-ID</Ref>
    </AssignVariable>
  	<AssignVariable>
        <Name>anthem.clientSecret</Name>
      <Ref>request.header.X-ANTHEM-CLIENT-SECRET</Ref>
    </AssignVariable>
  	<!--AssignVariable>
        <Name>anthem.basicAuth</Name>
      <Ref>request.header.Authorization</Ref>
    </AssignVariable-->
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
